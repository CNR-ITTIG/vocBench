<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY ittig "http://www.ittig.cnr.it/skos/aslt#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.ittig.cnr.it/ns/common/"
     xml:base="http://www.ittig.cnr.it/ns/common/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ittig="http://www.ittig.cnr.it/skos/aslt#">
    <owl:Ontology rdf:about="http://www.ittig.cnr.it/ns/common/"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ittig.cnr.it/ns/common/WBObjectProperty -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/WBObjectProperty">
        <rdfs:label xml:lang="en">Object Property</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Root Property for all object properties. Introduced in order to make querying and organization of properties easier.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/affects -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/affects">
        <rdfs:label xml:lang="en">Affects</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;affects&gt; Y. Agent X acts on object Y in such a way that Y changes state or location. E.g. &quot;sterilization&quot; &lt;affects&gt; &quot;bacteria&quot;; &quot;pest control&quot; &lt;affects&gt; &quot;pest&quot;; &quot;pollution of agriculture&quot; &lt;affects&gt; &quot;agriculture&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/causativeRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/belongsToCategory -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/belongsToCategory">
        <rdfs:label xml:lang="en">Belongs to category</rdfs:label>
        <rdfs:comment xml:lang="en">To map any domain concept to any category of any classification scheme.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_domain_concept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/belongsToScheme -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/belongsToScheme">
        <rdfs:label xml:lang="en">Belongs to scheme</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_classification_scheme"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/bibliographicRelationship -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/bibliographicRelationship">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Bibliographic relations</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/bibliographicRelationship"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/causativeRelationship -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/causativeRelationship">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Causative relationship</rdfs:label>
        <rdfs:comment xml:lang="en">All relationships producing an effect or being the effect of something. An expression of an agent causing or forcing a patient to perform an action (or to be in a certain condition). Functioning as an agent or cause.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/causativeRelationship"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/causes -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/causes">
        <rdfs:label xml:lang="en">Causes</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;causes&gt; Y. Agent X (animate or inanimate) brings about a result Y. Examples: &quot;prions&quot; &lt;causes&gt; &quot;BSE&quot;; &quot;water&quot; &lt;causes&gt; &quot;water erosion&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/causativeRelationship"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isCausedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/compose -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/compose">
        <rdfs:label xml:lang="en">Compose</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;compose&gt; X. A relation in which X composed of Y holds is one where Y consists of the material or substance of which X is made.  This relation also subsumes &lt;ingredientOf&gt; and &lt;substanceOf&gt; relations. E.g. &quot;blood gas&quot;, &quot;blood lipid&quot;, &quot;blood protein&quot;, &quot;blood cell&quot; &lt;composes&gt; &quot;blood&quot;; &quot;water&quot;, &quot;yeast&quot; &lt;compose&gt;  &quot;beer&quot;.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/correspondsTo -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/correspondsTo">
        <rdfs:label xml:lang="en">Corresponds to</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasAbbreviation -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasAbbreviation">
        <rdfs:label xml:lang="en">Has abbreviation</rdfs:label>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasAcronym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasAcronym">
        <rdfs:label xml:lang="en">Has acronym</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasAbbreviation"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isAcronymOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasAntonym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasAntonym">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has antonym or Has opposite</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasAntonym"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasBroaderSynonym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasBroaderSynonym">
        <rdfs:label xml:lang="en">Has broader synonym</rdfs:label>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasNarrowerSynonym"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasSynonym"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasCategory -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasCategory">
        <rdfs:label xml:lang="en">Has category</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/belongsToScheme"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_classification_scheme"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasComponent -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasComponent">
        <rdfs:label xml:lang="en">Has component</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;hasComponent&gt; X. An object X that is a part of a whole Y and has also an existence independently from Y. E.g. &quot;engine&quot; &lt;hasComponent&gt; &quot;engine part&quot;; &quot;tree&quot; &lt;hasComponent&gt; &quot;leaf&quot;; &quot;cell&quot; &lt;hasComponent&gt; &quot;chromosome&quot;; but NOT &quot;blood&quot; &lt;hasComponent&gt; &quot;blood cell&quot; (see &lt;composedOf&gt;).</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasDefinition -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasDefinition">
        <rdfs:label xml:lang="en">Definition</rdfs:label>
        <rdfs:label xml:lang="it">Definizione</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_definition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasGoalOrProcess -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasGoalOrProcess">
        <rdfs:label xml:lang="en">Has goal or process</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/instrumentalRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasLexicalization -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasLexicalization">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">Has lexicalization</rdfs:label>
        <rdfs:label xml:lang="it">Ha rappresentazione lessicale</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasMappedDomainConcept -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasMappedDomainConcept">
        <rdfs:label xml:lang="en">Has mapped domain concept</rdfs:label>
        <rdfs:comment xml:lang="en">to map domain concepts to any category in a classification scheme.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/belongsToCategory"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_domain_concept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasMember -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasMember">
        <rdfs:label xml:lang="en">Has member</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;hasMember&gt; X. A social or political group Y consists of one or more subsidiary social or political units (group or individual) X. E.g. &quot;herd&quot; &lt;hasMember&gt; &quot;cow&quot;; &quot;Francophone Africa&quot; &lt;hasMember&gt; &quot;Algeria&quot;, &quot;Benin&quot;, &quot;Burkina Faso&quot;, etc.;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasNarrowerSynonym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasNarrowerSynonym">
        <rdfs:label xml:lang="en">Has narrower synonym</rdfs:label>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasSynonym"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasNearSynonym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasNearSynonym">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has near synonym</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasNearSynonym"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasSynonym"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasOldName -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasOldName">
        <rdfs:label xml:lang="en">Has old name</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasSynonym"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasParent -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasParent">
        <rdfs:label xml:lang="en">Has parent</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasPart">
        <rdfs:label xml:lang="en">Has part</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;hasPart&gt; Y. A composite entity X that can be identified as composed by one or more parts, between which Y. Use this relationship when none of the other partitivity relations (&lt;component&gt;, &lt;composedOf&gt;, &lt;portion&gt;, &lt;member&gt;, &lt;includesSubprocess&gt;) apply. E.g. in a plant ontology: {Taxonomic} &lt;hasPart&gt; {PartPlant};</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasPortion -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasPortion">
        <rdfs:label xml:lang="en">Has portion</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;hasPortion&gt; Y. A mass X from which a piece Y can be taken. E.g. &quot;plant&quot; &lt;hasPortion&gt; &quot;cutting&quot;; &quot;chicken&quot; &lt;hasPortion&gt; &quot;chicken skin&quot;; &quot;a loaf of bread&quot; &lt;hasPortion&gt; &quot;a slice&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasPart"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isPortionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasRelatedConcept -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasRelatedConcept">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has related concept</rdfs:label>
        <rdfs:comment xml:lang="en">The most generic relationship to relate two domain concepts. This is used if two concepts are related and none of the more speciifc relationships can be applied. It is also used for backward compatibility to thesauri, i.e. all the more specific relationships can be resolved to this relationship and will translate to the &amp;lt;related term&amp;gt; or RT thesaurus relationship.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_domain_concept"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_domain_concept"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasRelatedTerm -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasRelatedTerm">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has related term</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasScopenoteRelatedToConcept -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasScopenoteRelatedToConcept">
        <rdfs:label xml:lang="en">Has scopenote related to concept</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasScopenoteRelatedToTerm -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasScopenoteRelatedToTerm">
        <rdfs:label xml:lang="en">Has scope note related to term</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isTermReferencedInScopenote"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasSynonym -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasSynonym">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has Synonym</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasSynonym"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/hasTranslation -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/hasTranslation">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Has translation</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasTranslation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/includes -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/includes">
        <rdfs:label xml:lang="en">Includes</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isIncludedIn"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/includesSubprocess -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/includesSubprocess">
        <rdfs:label xml:lang="en">Includes subprocess</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;includesSubprocess&gt; Y. Process X naturally or conventionally realized at the minimum through process Y. Equivalent to &lt;stageOf&gt;. E.g. &quot;milk production&quot; &lt;includesSubprocess&gt; &quot;pasteurization&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/includes"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/isSubprocessOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/instrumentalRelationship -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/instrumentalRelationship">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Instrumental relations</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates all relationships serving or acting as a means or aid. By means of an instrument or an agent. The means whereby some act is accomplished.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/instrumentalRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isAbbreviationOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isAbbreviationOf">
        <rdfs:label xml:lang="en">Is abbreviation of</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasAbbreviation"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isAcronymOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isAcronymOf">
        <rdfs:label xml:lang="en">Is acronym of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isAbbreviationOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isAffectedBy -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isAffectedBy">
        <rdfs:label xml:lang="en">Is affected by</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isAffectedBy&gt; X. An object Y changes state or location because of an action of an agent X. E.g. &quot;bacteria&quot; &lt;isAffectedBy&gt; &quot;sterilization&quot;; &quot;pest&quot; &lt;isAffectedBy&gt; &quot;pest control&quot;; &quot;agriculture&quot; &lt;isAffectedBy&gt; &quot;pollution of agriculture&quot;;</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/affects"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/causativeRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isCausedBy -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isCausedBy">
        <rdfs:label xml:lang="en">Is caused by</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isCausedBy&gt; X. A result Y occurred because of an agent X (animate or inanimate). E.g.: &quot;BSE&quot; &lt;isCausedBy&gt; &quot;prions&quot;; &quot;water erosion&quot; &lt;isCausedBy&gt; &quot;water&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/causativeRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isComponentOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isComponentOf">
        <rdfs:label xml:lang="en">Is component of</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;isComponentOf&gt; Y. An object X that is a part of a whole Y and has also an existence independently from Y. E.g. &quot;engine part&quot; &lt;isComponentOf&gt; &quot;engine&quot;; &quot;leaf&quot; &lt;isComponentOf&gt; &quot;tree&quot;; &quot;chromosome&quot; &lt;isComponentOf&gt; &quot;cell&quot;; but NOT &quot;blood cell&quot; &lt;isComponentOf&gt; &quot;blood&quot; (see &lt;composes&gt;).</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasComponent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isComposedOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isComposedOf">
        <rdfs:label xml:lang="en">Is composed of</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;isComposedOf&gt; Y. A relation in which X composed of Y holds is one where Y consists of the material or substance of which X is made.  This relation also subsumes &lt;ingredientOf&gt; and &lt;substanceOf&gt; relations. E.g. &quot;blood&quot; &lt;composedOf&gt; &quot;blood cell&quot;, &quot;&quot;blood gas&quot;, &quot;blood lipid&quot;, &quot;blood protein&quot;; &quot;beer&quot; &lt;composedOf&gt;  &quot;water&quot;, &quot;yeast&quot;, (and by inheritance &quot;hops&quot; c.f. &lt;madeFrom&gt;).</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/compose"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasPart"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isDefinitionOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isDefinitionOf">
        <rdfs:label xml:lang="en">Is definition of</rdfs:label>
        <rdfs:label xml:lang="it">E&#39; la definizione di</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_definition"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasDefinition"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isIncludedIn -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isIncludedIn">
        <rdfs:label xml:lang="en">Included in</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isLexicalizationOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isLexicalizationOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">Is a lexicalization of</rdfs:label>
        <rdfs:label xml:lang="it">E&#39; la rappresentazione lessicale di</rdfs:label>
        <rdfs:label xml:lang="it">E&#39; una lessicalizzazione di</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/WBObjectProperty"/>
        <rdfs:range rdf:resource="http://www.ittig.cnr.it/ns/common/c_category"/>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_lexicalization"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasLexicalization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isMemberOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isMemberOf">
        <rdfs:label xml:lang="en">Is member of</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;isMemberOf&gt; Y. A social or political unit (group or individual) XÂ belongs to a social or political group Y. E.g. &quot;cow&quot; &lt;isMemberOf&gt; &quot;herd&quot;; &quot;Algeria&quot;, &quot;Benin&quot;, &quot;Burkina Faso&quot;, etc. &lt;isMemberOf&gt; &quot;Francophone Africa&quot;.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasMember"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isParentOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isParentOf">
        <rdfs:label xml:lang="en">Is parent of</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasParent"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isPartOf">
        <rdfs:label xml:lang="en">Is part of</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isPartOf&gt; X. A part Y that compose an entity X. Use this relationship when none of the other partitivity relations (&lt;component&gt;, &lt;composedOf&gt;, &lt;portion&gt;, &lt;member&gt;, &lt;includesSubprocess&gt;) apply. E.g. in a plant ontology: {PlantPart} &lt;isPartOf&gt; {Taxonomic};</rdfs:comment>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasPart"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isPortionOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isPortionOf">
        <rdfs:label xml:lang="en">Is portion of</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isPortionOf&gt; X. Relation between a mass X and a piece Y taken from the mass. E.g. &quot;cutting&quot; &lt;isPortionOf&gt; &quot;plant&quot;; &quot;chicken skin&quot; &lt;isPortionOf&gt; &quot;chicken&quot;; &quot;a slice&quot; &lt;isPortionOf&gt; &quot;a loaf of bread&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isProcessFor -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isProcessFor">
        <rdfs:label xml:lang="en">Is process for</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;isProcessFor&gt; Y. One or more actions, activities, methods X that produce a change or development Y. E.g.: &quot;sterilization&quot; &lt;isProcessFor&gt; &quot;fruit cleaning&quot;.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasGoalOrProcess"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isReferencedInConceptScopenote -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isReferencedInConceptScopenote">
        <rdfs:label xml:lang="en">Is referenced in concept scopenote</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasScopenoteRelatedToConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isSpatiallyIncludedIn -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isSpatiallyIncludedIn">
        <rdfs:label xml:lang="en">Is spatially included in</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isSpatiallyIncludedIn&gt; X. Part X is an inalienable part of Y. E.g. &quot;Congo&quot; &lt;isSpatiallyIncludedIn&gt; &quot;Africa&quot;; &quot;Southeast Asia&quot; &lt;isSpatiallyIncludedIn&gt; &quot;Asia&quot;; &quot;hand&quot; &lt;isSpatiallyIncludedIn&gt; &quot;arm&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.ittig.cnr.it/ns/common/c_domain_concept"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isPartOf"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/spatiallyIncludes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isStudiedBy -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isStudiedBy">
        <rdfs:label xml:lang="en">Is studied by</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/bibliographicRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isSubprocessOf -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isSubprocessOf">
        <rdfs:label xml:lang="en">Is subprocess of</rdfs:label>
        <rdfs:comment xml:lang="en">Y &lt;isSubprocessOf&gt; X. Y is one of one or more processes naturally or conventionally associated with the realization of process X. E.g. &quot;pasteurization&quot; &lt;isSubprocessOf&gt; &quot;milk production&quot;;</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/isIncludedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/isTermReferencedInScopenote -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/isTermReferencedInScopenote">
        <rdfs:label xml:lang="en">Is term referenced in scope note</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/partitiveRelationship -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/partitiveRelationship">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Partitive relations</rdfs:label>
        <rdfs:comment xml:lang="en">All partitive relationships.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/partitiveRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/quantitativeRelationship -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/quantitativeRelationship">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Quantitative relationship</rdfs:label>
        <rdfs:comment xml:lang="en">All relationships involving a quantity or a measure.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasRelatedConcept"/>
        <owl:inverseOf rdf:resource="http://www.ittig.cnr.it/ns/common/quantitativeRelationship"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ittig.cnr.it/ns/common/spatiallyIncludes -->

    <owl:ObjectProperty rdf:about="http://www.ittig.cnr.it/ns/common/spatiallyIncludes">
        <rdfs:label xml:lang="en">Spatially includes</rdfs:label>
        <rdfs:comment xml:lang="en">X &lt;spatiallyIncludes&gt; Y. Part X is an inalienable part of Y (the spatial region occupied by Y is a  part of the spatial region occupied by X). E.g. &quot;Africa&quot; &lt;spatiallyIncludes&gt; &quot;Congo&quot;; &quot;Asia&quot; &lt;spatiallyIncludes&gt; &quot;Southeast Asia&quot;; &quot;arm&quot; &lt;spatiallyIncludes&gt; &quot;hand&quot;.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.ittig.cnr.it/ns/common/hasPart"/>
    </owl:ObjectProperty>
    


